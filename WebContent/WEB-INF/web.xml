<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="webseer" version="2.5">
  <session-config>
    <session-timeout>60</session-timeout>
  </session-config>
  <display-name>webseer-old</display-name>
  <welcome-file-list>
    <welcome-file>index</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>UrlRewriteFilter</filter-name>
    <filter-class>org.tuckey.web.filters.urlrewrite.UrlRewriteFilter</filter-class>
    <init-param>
      <param-name>logLevel</param-name>
      <param-value>WARN</param-value>
    </init-param>
  </filter>
  <filter>
    <filter-name>LoggedInFilter</filter-name>
    <filter-class>org.webseer.web.LoggedInFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>LoggedInFilter</filter-name>
    <url-pattern>/workspace/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
    <filter-name>UrlRewriteFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>org.webseer.web.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>org.webseer.web.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>index</servlet-name>
    <servlet-class>org.webseer.web.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>user</servlet-name>
    <servlet-class>org.webseer.web.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>resetPassword</servlet-name>
    <servlet-class>org.webseer.web.PasswordResetServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>edit-workspace</servlet-name>
    <servlet-class>org.webseer.web.EditWorkspace</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>delete-workspace</servlet-name>
    <servlet-class>org.webseer.web.DeleteWorkspace</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>create-workspace</servlet-name>
    <servlet-class>org.webseer.web.CreateWorkspace</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getLanguage-workspace</servlet-name>
    <servlet-class>org.webseer.web.scripts.TransformationLanguageScript</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>modify-workspace-graph</servlet-name>
    <servlet-class>org.webseer.web.ajax.ModifyWorkspaceGraph</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>render-node</servlet-name>
    <servlet-class>org.webseer.web.RenderNode</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>uploadFileTo-workspace</servlet-name>
    <servlet-class>org.webseer.web.UploadFileToWorkspace</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getFileFrom-workspace</servlet-name>
    <servlet-class>org.webseer.web.GetFileFromWorkspace</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getFileFrom-workspace</servlet-name>
    <url-pattern>/getFileFrom-workspace</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>uploadFileTo-workspace</servlet-name>
    <url-pattern>/uploadFileTo-workspace</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>modify-workspace-graph</servlet-name>
    <url-pattern>/modify-workspace</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>render-node</servlet-name>
    <url-pattern>/render-workspace</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getLanguage-workspace</servlet-name>
    <url-pattern>/getLanguage-workspace</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>create-workspace</servlet-name>
    <url-pattern>/createWorkspace</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>delete-workspace</servlet-name>
    <url-pattern>/delete-workspace</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>edit-workspace</servlet-name>
    <url-pattern>/edit-workspace</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>user</servlet-name>
    <url-pattern>/user</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>resetPassword</servlet-name>
    <url-pattern>/resetPassword</url-pattern>
  </servlet-mapping>
</web-app>